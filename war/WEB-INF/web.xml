<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>UploadRequestServlet</servlet-name>
		<servlet-class>edu.dartmouth.cs.dartcardserver.UploadRequestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadRequestServlet</servlet-name>
		<url-pattern>/blob/getuploadurl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-class>edu.dartmouth.cs.dartcardserver.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadServlet</servlet-name>
		<url-pattern>/blob/upload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GeographyServlet</servlet-name>
		<servlet-class>edu.dartmouth.cs.dartcardserver.GeographyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GeographyServlet</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>QueryServlet</servlet-name>
		<servlet-class>edu.dartmouth.cs.dartcardserver.QueryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryServlet</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
</web-app>
